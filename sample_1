import re

def format_strings(input_list):
    output_list = []
    for item in input_list:
        formatted = re.sub(r'([A-Z])([A-Z][a-z])', r'\1 \2', item)
        formatted = re.sub(r'([a-z])([A-Z])', r'\1 \2', formatted)
        formatted = re.sub(r'(\d+)', r' \1', formatted)
        output_list.append(formatted)
    return output_list

# Example usage
input_values = [
    "DEMONSTRATIONPROJECTIDENTIFIER",
    "DIAGNOSISTYPECODE1",
    "O2SATURATEDQUALITY",
    "CONDITIONRESPONSECODE"
]

output_values = format_strings(input_values)
for output in output_values:
    print(output)
