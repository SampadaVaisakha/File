import re
import nltk
from nltk.corpus import words
nltk.download('words')

def format_strings(input_list):
    word_list = set(words.words())
    output_list = []
    
    for item in input_list:
        start = 0
        formatted = ""
        
        for end in range(1, len(item) + 1):
            if item[start:end] in word_list or end == len(item):
                formatted += item[start:end] + " "
                start = end
        
        output_list.append(formatted.strip())
    return output_list

# Example usage
input_values = [
    "DEMONSTRATIONPROJECTIDENTIFIER",
    "DIAGNOSISTYPECODE1",
    "O2SATURATEDQUALITY",
    "CONDITIONRESPONSECODE"
]

output_values = format_strings(input_values)
for output in output_values:
    print(output)
